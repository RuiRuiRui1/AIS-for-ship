        			<ScaleTransform/>
        			<SkewTransform/>
        			<RotateTransform Angle="90"/>
        			<TranslateTransform/>
        		</TransformGroup>
        	</ed:Arc.RenderTransform>
        </ed:Arc>
        <ed:Arc x:Name="BorderPart3" ArcThickness="0" ArcThicknessUnit="Pixel" EndAngle="90" Fill="{x:Null}" HorizontalAlignment="Left" Margin="6.404,12.366,0,17.633" Stretch="None" Stroke="Black" StartAngle="0" Width="1" RenderTransformOrigin="0.5,0.5" d:LayoutOverrides="HorizontalAlignment, VerticalAlignment, VerticalMargin" Height="10" Visibility="Collapsed" >
        	<ed:Arc.RenderTransform>
        		<TransformGroup>
        			<ScaleTransform/>
        			<SkewTransform/>
