        </ed:Arc>
        <ed:Arc x:Name="BorderPart4" ArcThickness="0" ArcThicknessUnit="Pixel" EndAngle="90" Fill="{x:Null}" HorizontalAlignment="Left" Stretch="None" Stroke="Black" StartAngle="0" Width="1" d:LayoutOverrides="HorizontalAlignment, VerticalAlignment, VerticalMargin" Height="10" Margin="6.87,16.864,0,13.135" Visibility="Collapsed" />
        <ed:Arc x:Name="BorderPart5" ArcThickness="0" ArcThicknessUnit="Pixel" EndAngle="90" Fill="{x:Null}" HorizontalAlignment="Left" Margin="7.204,34.334,0,-4.335" Stretch="None" Stroke="Black" StartAngle="0" Width="1" d:LayoutOverrides="HorizontalAlignment, VerticalMargin" Height="10" Visibility="Collapsed" />
        <ed:Arc x:Name="BorderPart6" ArcThickness="0" ArcThicknessUnit="Pixel" EndAngle="90" Fill="{x:Null}" HorizontalAlignment="Left" Margin="7.003,0,0,-3.867" Stretch="None" Stroke="Black" StartAngle="0" Width="1" RenderTransformOrigin="0.5,0.5" d:LayoutOverrides="HorizontalAlignment" Height="10" VerticalAlignment="Bottom" Visibility="Collapsed" >
        	<ed:Arc.RenderTransform>
        		<TransformGroup>
        			<ScaleTransform/>
        			<SkewTransform/>
        			<RotateTransform Angle="90"/>
        			<TranslateTransform/>
        		</TransformGroup>
        	</ed:Arc.RenderTransform>
        </ed:Arc>
        <ed:Arc x:Name="BorderPart7" ArcThickness="0" ArcThicknessUnit="Pixel" EndAngle="90" Fill="{x:Null}" HorizontalAlignment="Left" Margin="31.332,33.933,0,-3.934" Stretch="None" Stroke="Black" StartAngle="0" Width="1" Height="10" d:LayoutOverrides="VerticalMargin" Visibility="Collapsed" />
        <ed:Arc x:Name="BorderPart8" ArcThickness="0" ArcThicknessUnit="Pixel" EndAngle="90" Fill="{x:Null}" HorizontalAlignment="Left" Margin="27.399,0,0,-3.8" Stretch="None" Stroke="Black" StartAngle="0" Width="1" RenderTransformOrigin="0.5,0.5" Height="10" VerticalAlignment="Bottom" Visibility="Collapsed" >
        	<ed:Arc.RenderTransform>
        		<TransformGroup>
        			<ScaleTransform/>
        			<SkewTransform/>
        			<RotateTransform Angle="90"/>
        			<TranslateTransform/>
        		</TransformGroup>
        	</ed:Arc.RenderTransform>
        </ed:Arc>
    </Grid>
</UserControl>
