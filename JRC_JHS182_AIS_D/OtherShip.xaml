                " Margin="19.5,0,0,0" Stretch="None" Stroke="Black" StartAngle="0" VerticalAlignment="Center" Width="1" />
        <ed:RegularPolygon   Height="20" HorizontalAlignment="Left" InnerRadius="1" Margin="15,20,0,0" x:Name="Ship" PointCount="3" RenderTransformOrigin="3.286,4.286" Stretch="Fill" VerticalAlignment="Center" Width="10" Stroke="Black" Opacity="1" OpacityMask="{x:Null}" Fill="{x:Null}" />
